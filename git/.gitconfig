[user]
	email = goweiwen@gmail.com
	name = Goh Wei Wen

[color]
	ui = always

[core]
	editor = nvim

[alias]
	a = add
	b = branch
	bc = checkout -b
	c = commit
	ca = commit --amend
	cac = commit --amend -C HEAD
	can = commit --amend --no-edit
	co = checkout
	d = diff
	dw = diff --color-words
	fc = clone
	fm = pull --merge
	fr = pull --rebase
	l = log --oneline
	ll = log
	p = push
	pf = push --force-with-lease
	pt = push --tags
	pu = push --set-upstream origin HEAD
	r = rebase
	ra = rebase --abort
	rc = rebase --continue
	rs = rebase --skip
	s = status --short
	ss = status
